    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0",
    "gpt-tokenizer": "^3.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tiktoken": "^1.0.22",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
